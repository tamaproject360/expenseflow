    "build:ios:local": "expo prebuild --platform ios && cd ios && pod install",
    "lint": "expo lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean"
